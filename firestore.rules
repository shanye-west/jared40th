rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Public read
    match /{document=**} {
      allow read: if true;
    }
    // TEMP: authenticated users can update any field on matches
    match /matches/{matchId} {
      allow update: if request.auth != null;
      allow create, delete: if false;
    }
  }
}
